{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "dashng"
  },
  "apps": [
    {
      "name": "dashng",
      "root": "src/dashng",
      "outDir": "dist/dashng",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "index.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.dashng.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "dashng",
      "styles": [
        "styles.css"
      ],
      "scripts": [],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    },
    {
      "name": "dashngtest",
      "root": "src/dashngtest",
      "outDir": "dist/dashngtest",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.css"
      ],
      "scripts": [],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/dashng/tsconfig.dashng.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/dashng/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/dashngtest/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/dashngtest/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
